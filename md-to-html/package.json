{
  "name": "markdown-it-synapse-lambda",
  "version": "1.0.1",
  "description": "Plugin to create a node js lambda to resolve Synapse markdown to html",
  "repository": "sage-bionetworks/markdown-it-lambda-fct",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "start": "node ./bin/www"
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "Synapse"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-plugin-nodeca": "^1.0.0",
    "html-to-text": "^8.0.0",
    "juice": "^7.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-br": "^1.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-container": "^4.0.0",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sub-alt": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "markdown-it-synapse": "^1.1.16",
    "markdown-it-synapse-heading": "^1.0.1",
    "markdown-it-synapse-math": "^3.0.5",
    "markdown-it-synapse-table": "^1.0.6",
    "markdown-it-testgen": "^0.1.0",
    "mocha": "^9.1.3",
    "morgan": "^1.10.0",
    "pug": "^3.0.3",
    "serve-favicon": "^2.5.0",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "transform": {},
    "testMatch": [ 
      "**/spec/**/*.js?(x)", "**/?(*.)(spec|test).js?(x)", 
      "**/spec/**/*.mjs", "**/?(*.)(spec|test).mjs" 
    ]
  }
}
